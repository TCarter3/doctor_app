<h1>Users</h1>
<%= link_to 'Add New User', new_user_path %>
<hr/>

<ul class="ui middle aligned animated list">
    <% @users.each do |user| %>
        <div class="item">
        <li>
            <div class="header">
            <%= link_to user.full_name, user_path(user) %>
            </div>
        </li>
        </div>
    <% end %>
</ul>
