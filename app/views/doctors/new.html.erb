<h1>Add Doctor</h1>
<hr/>
<%= form_with(model: @doctor, local: true, class: "ui form") do |f| %>
    <div class="field">
        <%= f.label :name %>
        <%= f.text_field :name, required: true, placeholder: "Doctor Name" %>
    </div>
        <%= f.submit class: "ui green button inverted" %>
    <% end %>


